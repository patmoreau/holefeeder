{
  "name": "holefeeder-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "serve": "ng serve --configuration development",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44418 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44418 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "analyze": "source-map-explorer dist/**/*.js",
    "watch": "ng build --watch --configuration development",
    "mock:server": "json-server mocks/data.json --routes mocks/routes.json --watch -m ./node_modules/json-server-auth",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "ng test --watch=true --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
