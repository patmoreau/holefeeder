{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter: web",
            "type": "dart",
            "request": "launch",
            "deviceId": "chrome",
            "args": [
                "--web-port",
                "3000"
            ],
            "cwd": "${workspaceFolder}/src/Holefeeder.Ui"
        },
        {
            "name": "flutter",
            "type": "dart",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/Holefeeder.Ui"
        },
        {
            "name": "Holefeeder.Ui",
            "type": "dart",
            "request": "launch",
            "program": "${workspaceFolder}/src/Holefeeder.Ui/lib/main.dart"
        },
        {
            "name": "C#: Holefeeder.Api Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/src/Holefeeder.Api/Holefeeder.Api.csproj"
        },
    ],
    "compounds": []
}